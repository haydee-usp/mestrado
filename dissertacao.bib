mendeley-tags = {genero,mestrado,poli},
month = jul,
pages = {1--10},
title = {{Valuing perceived insecurity associated with use of and access to public transport}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0967070X12000704},
volume = {22},
year = {2012}
}
@article{Evenson2007,
abstract = {OBJECTIVES: To describe the development of an original scale that measures perceived school climate for active travel in fourth- and fifth-grade girls and boys. METHODS: The data were analyzed using confirmatory factor analysis (CFA) to provide evidence of factorial validity, factorial invariance, and construct validity. RESULTS: The CFA supported the fit of a 3-factor (encouragement, praise, and importance) correlated model for the school climate for active travel measure. This hierarchical model was invariant between sex and across a 7-month time period, and initial evidence for construct validity was provided. CONCLUSIONS: School climate for active travel is a measurable construct, and preliminary evidence suggests relationships with more support for active travel from friends and family.},
