month = dec,
pages = {54--61},
title = {{Gender and the Automobile: Analysis of Nonwork Service Trips}},
url = {http://trb.metapress.com/openurl.asp?genre=article\&id=doi:10.3141/2013-08},
volume = {2013},
year = {2007}
}
